<!DOCTYPE html>
<html lang="pt">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}{% endblock %} | Consola de Gest√£o</title>
        {% image '@XvolutionsAdminBundle/Resources/public/images/favicon.ico' %}
        <link rel="shortcut icon" href="{{ asset_url }}" />
        {% endimage %}
        {% stylesheets 
            '@XvolutionsAdminBundle/Resources/public/css/bootstrap.css'
        %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}">
        {% endstylesheets %}
    {% block styles %}{% endblock %}
</head>
<body>
    <div id="wrapper">
        <div class="container-fluid">
            <div class="content">
                <div class="row">
                {% block content %}{% endblock %}
                </div>
            </div>
        </div>
    </div>

<!-- Javascript -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
{% javascripts 
                    '@XvolutionsAdminBundle/Resources/public/js/jquery-1.11.0.min.js' 
                    '@XvolutionsAdminBundle/Resources/public/js/bootstrap.min.js' 
                    '@XvolutionsAdminBundle/Resources/public/js/selectforremoval.js' 
                    '@XvolutionsAdminBundle/Resources/public/js/delete.js' 
                    '@XvolutionsAdminBundle/Resources/public/js/hide_alerts.js' 
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% block javascripts %}{% endblock %}
<!-- Javascript -->
</body>
</html>